@page


@model LawyerAreaOfActivitiesModel
@{
    ViewData["Title"] = "Area Of Activities";
    ViewData["ActivePage"] = ManageNavPages.LawyerAreaOfActivities;
}
<h3>@ViewData["Title"]</h3>

@foreach (var item in Model.AreasOfActivitys)
{
                <div class="form-check">
        <input class="form-check-input" type="checkbox" value="@item.Id" id="flexCheckDefault">
        <label class="form-check-label" for="flexCheckDefault">
            @item.Name
        </label>
    </div>
    
    
}
@section Scripts {
<partial name="_ValidationScriptsPartial" />
}
